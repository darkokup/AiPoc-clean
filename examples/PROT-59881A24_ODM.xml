        <StudyEventRef StudyEventOID="SE.V7" OrderNumber="8" Mandatory="Yes"/>
        <Arm OID="ARM.1" Name="Intervention">
          <Description>
            <TranslatedText xml:lang="en">Intervention</TranslatedText>
          </Description>
        </Arm>
        <Arm OID="ARM.2" Name="Control">
          <Description>
