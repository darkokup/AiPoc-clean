          </Description>
        </Arm>
        <InclusionExclusionCriteria>
          <Criterion OID="IC.1" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">Age between 18 and 75 years at the time of consent.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.2" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">Documented diagnosis of Type 2 Diabetes Mellitus for at least 6 months prior to screening.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.3" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">HbA1c level between 7.0% and 10.0% at screening.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.4" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">Body Mass Index (BMI) between 25.0 kg/m² and 40.0 kg/m².</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.5" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">Currently on stable doses of metformin for at least 3 months prior to screening.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.6" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">Able and willing to provide written informed consent prior to any study-related procedures.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.7" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">Fasting plasma glucose between 126 mg/dL and 200 mg/dL at screening.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.8" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">No significant changes in lifestyle (diet or exercise) anticipated during the study period.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="IC.9" Type="Inclusion">
            <Description>
              <TranslatedText xml:lang="en">Willing to comply with study procedures, including regular clinic visits and laboratory tests.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.1" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Patients with a history of severe hypoglycemic episodes in the past 6 months.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.2" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Individuals with a known allergy to the investigational drug or its components.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.3" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Participants currently taking medication that significantly affects glucose metabolism, unless discontinued at least 4 weeks prior to the study.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.4" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Subjects with significant renal impairment (eGFR &lt; 30 mL/min/1.73 m2).</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.5" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Patients with active liver disease or significant liver enzyme elevation (ALT or AST &gt; 3 times the upper limit of normal).</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.6" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Individuals with a history of cardiovascular events such as myocardial infarction or stroke within the previous 6 months.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.7" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Pregnant or breastfeeding women, or those planning to become pregnant during the study period.</TranslatedText>
            </Description>
          </Criterion>
          <Criterion OID="EC.8" Type="Exclusion">
            <Description>
              <TranslatedText xml:lang="en">Participants currently enrolled in another clinical trial or who have participated in a clinical trial within the last 30 days.</TranslatedText>
            </Description>
          </Criterion>
        </InclusionExclusionCriteria>
